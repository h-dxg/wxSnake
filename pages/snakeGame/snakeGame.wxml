<canvas canvas-id="myCanvas" style="width: 100%; height: 65%; background-color: #000"></canvas>
<text class="grades">分数：{{grades}}</text>
<view class="btn">
  <button size="mini" bindtap="save" style="margin-bottom:50rpx">Start</button>
</view>
<view class="btn">
  <button size="mini" bindtap="up">上</button>
  <view class="cet">
    <button size="mini" style="margin-right:20rpx" bindtap="left">左</button>
    <button size="mini" style="margin-left:20rpx" bindtap="right">右</button>
  </view>
  <button size="mini" bindtap="down">下</button>
</view>